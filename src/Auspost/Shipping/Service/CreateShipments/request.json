{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "CreateShipments",
    "description": "This interface creates a shipment with items and returns a summary of the pricing for the items.",
    "type": "object",
    "properties": {
        "shipments": {
            "type": "array",
            "items": {
                "properties": {
                    "shipment_id": {
                        "description": "The identifier for the shipment generated by Australia Post.",
                        "type": "string"
                    },
                    "length": {
                        "description": "The length of the parcel in centimetres. If volumetric pricing applies then this is mandatory.",
                        "type": "number"
                    },
                    "width": {
                        "description": "The width of the parcel in centimetres. If volumetric pricing applies then this is mandatory.",
                        "type": "number"
                    },
                    "height": {
                        "description": "The height of the parcel in centimetres. If volumetric pricing applies then this is mandatory.",
                        "type": "number"
                    },
                    "weight": {
                        "description": "The parcel's weight in kilograms.",
                        "type": "number"
                    },
                    "packaging_type": {
                    
                    },
                    "product_ids": {
                    
                    },
                    "features": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "description": "Type of additional feature. Eg: TRANSIT_COVER.",
                                "type": "string"
                            },
                            "attributes": {
                                "description": "Additional information about the product. Please note that feature attributes depends on the feature type.",
                                "type": "object",
                                "properties": {
                                    "cover_amount": {
    									"description": "The amount of cover required.",
                                        "type": "number"
                                    }
                                }
                            }
                        }
                    },
                    "required": ["type"]
                }
            }
        }
    },
    "required": ["from","to"]
}